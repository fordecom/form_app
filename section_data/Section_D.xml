<?xml version="1.0" encoding="UTF-8"?>

<section index="4" title="annual_charges">
    
    <subsection index="1" title="pension_payments" description="Pensions, Annual Payments and Charges">
        <!-->**************************************************************************************************************</!-->
        <question type="select" default="false" name="p_payments" description="Did you make any pension payments in the year" question_index="9">
            <option value="true">Yes: Self</option>
            <option value="true">Yes: Spouse</option>
            <option value="false">No</option>
        </question>
        <question type="field_group" name="p_payments_info" description="If yes, please answer the following">
            <dependencies>
                <dependency data="hide" reference="p_payments" value="true" />
            </dependencies>
            <question type="select" default="false" name="pp_plan" description="Do you have a Personal Pension Plan (Retirement Annuity Contributions)" question_index="9">
                <option value="true">Yes: Self</option>
                <option value="true">Yes: Spouse</option>
                <option value="false">No</option>
            </question>
            <question type="field_group" name="pp_plan_info" description="If yes, please answer the following">
                <dependencies>
                    <dependency data="hide" reference="pp_plan" value="true" />
                </dependencies>              
                <question type="select" default="false" name="confirm_contributions" description="Confirm your contributions to a Personal Pension Plan for 2012.">
                    <option value="true">Information Attached</option>
                    <option value="false">N/A</option>
                </question>
                <question type="select" default="false" name="confirm_relief" description="Confirm if tax relief has been claimed on the above contributions in your 2011 tax return.">
                    <option value="true">Information Attached</option>
                    <option value="false">N/A</option>
                </question>
                <question type="select" default="false" name="confirm_pp_plan" description="Confirm your contributions to a Personal Pension Plan for 1 Jan to 31 Oct 2013.">
                    <option value="true">Information Attached</option>
                    <option value="false">N/A</option>
                </question>
            </question>
            <question type="select" default="false" name="prsa" description="Do you have a PRSA" question_index="9">
                <option value="true">Yes: Self</option>
                <option value="true">Yes: Spouse</option>
                <option value="false">No</option>
            </question>
            <question type="field_group" name="prsa_info" description="If yes, please answer the following">
                <dependencies>
                    <dependency data="hide" reference="prsa" value="true" />
                </dependencies> 
                <question type="select" default="false" name="contributions_to_prsa" description="Confirm your contributions to a PRSA for 2012.">
                    <option value="true">Information Attached</option>
                    <option value="false">N/A</option>
                </question>
                <question type="select" default="false" name="employer_prsa" description="Confirm contributions made by your employer to your PRSA for 2012.">
                    <option value="true">Information Attached</option>
                    <option value="false">N/A</option>
                </question>
                <question type="select" default="false" name="payroll_prsa" description="Confirm if tax relief has been claimed on any of the above contributions either in your 2011 tax return or through your payroll.">
                    <option value="true">Information Attached</option>
                    <option value="false">N/A</option>
                </question>
                <question type="select" default="false" name="contributions_to_prsa_ny" description="Confirm your contributions to a PRSA for 1 Jan to 31 Oct 2013.">
                    <option value="true">Information Attached</option>
                    <option value="false">N/A</option>
                </question>
            </question>
            <question type="select" default="false" name="opcavc" description="Do you have an Occupational Pension Contributions &amp; AVC" question_index="9">
                <option value="true">Yes: Self</option>
                <option value="true">Yes: Spouse</option>
                <option value="false">No</option>
            </question>
            <question type="field_group" name="opcavc_info" description="If yes, please answer the following">
                <dependencies>
                    <dependency data="hide" reference="opcavc" value="true" />
                </dependencies>
                <question type="select" default="false" name="contributions_to_ops" description="Confirm your contributions to an occupational pension scheme for 2012">
                    <option value="true">Information Attached</option>
                    <option value="false">N/A</option>
                </question>
                <question type="select" default="false" name="additional_voluntary" description="Confirm Additional Voluntary Contributions made by you in 2012">
                    <option value="true">Information Attached</option>
                    <option value="false">N/A</option>
                </question>
                <question type="select" default="false" name="confirm_if_any" description="Confirm if tax relief has been claimed on any of the above contributions either in your 2011 tax return or through your payroll.">
                    <option value="true">Information Attached</option>
                    <option value="false">N/A</option>
                </question>
                <question type="select" default="false" name="cyc_to_an_avc" description="Confirm your contributions to an AVC or occupational pension scheme for 1 Jan to 31 Oct 2013 for which you have not already claimed tax relief through your payroll.">
                    <option value="true">Information Attached</option>
                    <option value="false">N/A</option>
                </question>
            </question>
            <!-->**************************************************************************************************************</!-->
        </question>
    </subsection>
    
    <subsection index="2" title="annuities_maintenance_deeds">
        <!-->**************************************************************************************************************</!-->
        <question type="select" default="false" name="amp_deeds" description="Did you pay any Annuities, Maintenance Payments, Deeds of Covenant Information" question_index="9">
            <option value="true">Yes: Self</option>
            <option value="true">Yes: Spouse</option>
            <option value="false">No</option>
        </question>
        <question type="field_group" name="amp_deeds_info" description="If yes, please answer the following">
            <dependencies>
                <dependency data="hide" reference="amp_deeds" value="true" />
            </dependencies>
            <question type="select" default="false" name="t_o_payment" description="(a) Type of Payment">
                <option value="true">Information Attached</option>
                <option value="false">N/A</option>
            </question>
            <question type="select" default="false" name="t_w_i_i_paid" description="(b) To Whom it is Paid">
                <option value="true">Information Attached</option>
                <option value="false">N/A</option>
            </question>
            <question type="select" default="false" name="a_o_recipient" description="(c) Address of Recipient">
                <option value="true">Information Attached</option>
                <option value="false">N/A</option>
            </question>
            <question type="select" default="false" name="g_a_paid" description="(d) Gross Amount Paid in 2012">
                <option value="true">Information Attached</option>
                <option value="false">N/A</option>
            </question>
            <question type="select" default="false" name="r_o_r_t_payer" description="(e) Relationship of recipient to payer (if any)">
                <option value="true">Information Attached</option>
                <option value="false">N/A</option>
            </question>
            <question type="select" default="false" name="d_o_t_deducted" description="(f) Details of tax deducted (if any)">
                <option value="true">Information Attached</option>
                <option value="false">N/A</option>
            </question>
            
            <!-->**************************************************************************************************************</!-->
        </question>
    </subsection>
    
    <subsection index="3" title="other_interest_payments">
        <!-->**************************************************************************************************************</!-->
        <question type="select" default="false" name="oi_payments" description="Did you pay any interest (Other than mortgage interest)" question_index="9">
            <option value="true">Yes: Self</option>
            <option value="true">Yes: Spouse</option>
            <option value="false">No</option>
        </question>
        <question type="field_group" name="oi_payments_info" description="If yes, please answer the following">
            <dependencies>
                <dependency data="hide" reference="oi_payments" value="true" />
            </dependencies>
            <question type="select" default="false" name="taken_out_prior" description="For loans taken out prior to 7 December 2010, certificates of interest paid in 2012 on loans applied in acquiring an interest in or lending money to an unquoted trading company.">
                <option value="true">Information Attached</option>
                <option value="false">N/A</option>
            </question>
            <question type="select" default="false" name="loans_applied_in_a" description="Certificates of interest paid in 2012 on loans applied in acquiring an interest in or lending money to a partnership">
                <option value="true">Information Attached</option>
                <option value="false">N/A</option>
            </question>
            <question type="select" default="false" name="original_amount_o_l" description="Original amount of loan and date the loan was taken out">
                <option value="true">Information Attached</option>
                <option value="false">N/A</option>
            </question>
            <question type="select" default="false" name="d_o_a_c_recovered" description="Details of any capital recovered from the partnership or company not used to reduce the loan on which interest relief is claimed.">
                <option value="true">Information Attached</option>
                <option value="false">N/A</option>
            </question>
            
            <!-->**************************************************************************************************************</!-->
        </question>
    </subsection>
    
    <subsection index="4" title="rents_to_non_residential_landlord">
        <!-->**************************************************************************************************************</!-->
        <question type="select" default="false" name="rtnr_landlord" description="Did you pay any rents to Non-Resident Landlord Information" question_index="9">
            <option value="true">Yes: Self</option>
            <option value="true">Yes: Spouse</option>
            <option value="false">No</option>
        </question>
        <question type="field_group" name="rtnr_landlord_info" description="If yes, please answer the following">
            <dependencies>
                <dependency data="hide" reference="rtnr_landlord" value="true" />
            </dependencies>
            
            <question type="select" default="false" name="r_p_t_a_non_r_l" description="Provide details of rents paid to a non-resident landlord.">
                <option value="true">Information Attached</option>
                <option value="false">N/A</option>
            </question>
            
            <question type="select" default="false" name="wtortt_landlord" description="Did you withhold tax on rents to the landlord and remit it to Revenue?" question_index="9">
                <option value="false">Yes: Self</option>
                <option value="false">Yes: Spouse</option>
                <option value="true">No</option>
            </question>
            <question type="field_group" name="wtortt_landlord_info">
                <dependencies>
                    <dependency data="hide" reference="wtortt_landlord" value="false" />
                </dependencies>
                <question type="select" default="false" name="landlord_appoint" description="If no, did the Landlord appoint an agent in Ireland to collect the rent?" question_index="9">
                    <option value="true">Yes: Self</option>
                    <option value="true">Yes: Spouse</option>
                    <option value="false">No</option>
                </question>
            </question>
            <!-->**************************************************************************************************************</!-->
        </question>
    </subsection>
    
